{
  "application": {
    "name": "DataMD Processor",
    "version": "1.0.0",
    "environment": "production"
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "name": "datamd_db",
    "ssl": true
  },
  "features": {
    "ocr_enabled": true,
    "pdf_processing": true,
    "video_support": true,
    "excel_formats": ["xlsx", "xls", "xlsm", "ods"]
  },
  "limits": {
    "max_file_size_mb": 100,
    "max_pages_pdf": 50,
    "supported_languages": ["eng", "spa", "fra", "deu"]
  }
}
